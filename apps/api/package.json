  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@repo/vitest-config": "workspace:*",
    "@types/express": "^5.0.5",
    "@types/node": "*",
    "@vitest/coverage-v8": "4.0.14",
    "tsconfig-paths": "^4.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "*"
  }
}
